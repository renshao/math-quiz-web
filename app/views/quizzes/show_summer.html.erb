<div class="header">
  <span>QUIZ ID - <%= @quiz_id %></span>
  <span class="date"><%= Time.now.in_time_zone('Australia/Sydney').strftime("%^A, %d/%m/%Y") %></span>
</div>

<div class="questions-section summer">
  <% @questions[0, 5].each do |q| %>
    <div class="question summer"><%= q[0] %> <%= q[1] %> <%= q[2] %> = <span class="answer"><%= q[3] %></span></div>
  <% end %>
</div>

<div class="questions-section summer">
  <% @questions[5, 5].each do |q| %>
    <div class="question summer"><%= q[0] %> <%= q[1] %> <%= q[2] %> = <span class="answer"><%= q[3] %></span></div>
  <% end %>
</div>

